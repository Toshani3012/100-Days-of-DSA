#include <stdio.h>

int removeDuplicates(int nums[], int n) {
    if (n == 0) return 0;

    int i = 0; // pointer for last unique element
    for (int j = 1; j < n; j++) {
        if (nums[j] != nums[i]) {
            i++;
            nums[i] = nums[j];
        }
    }
    return i + 1; // new length of unique array
}

int main() {
    int nums[] = {1, 1, 2, 2, 3, 4, 4};
    int n = sizeof(nums) / sizeof(nums[0]);

    int length = removeDuplicates(nums, n);

    printf("New length: %d\n", length);
    printf("Array after removing duplicates: ");
    for (int k = 0; k < length; k++) {
        printf("%d ", nums[k]);
    }
    printf("\n");

    return 0;
}
