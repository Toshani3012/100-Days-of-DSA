#include <stdio.h>

int main() {
    int arr[10] = {1, 2, 3, 4, 5};
    int n = 5; // current size
    int pos = 2; // index where to insert
    int element = 99;

    // shift elements to the right
    for (int i = n; i > pos; i--) {
        arr[i] = arr[i - 1];
    }

    arr[pos] = element;
    n++; // increase size

    // print updated array
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);
    }
    return 0;
}
